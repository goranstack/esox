<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>nu.esox</groupId>
	<artifactId>esoxCore</artifactId>
	<name>Esox Core</name>
	<version>0.0.1-SNAPSHOT</version>
	<description>Esox Core contains classes for implementing the model part of the aspect binding.</description>
	<url>http://code.google.com/p/esox</url>
	<scm>
		<connection>http://esox.googlecode.com/svn/trunk/esoxCore</connection>
		<developerConnection>https://esox.googlecode.com/svn/trunk/esoxCore</developerConnection>
	</scm>
	<build>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>